{
  "name": "imran-accordian",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://www.imransarwar.co.uk/imran-accordian",
  "dependencies": {
    "cypress": "^3.3.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest-styled-components": "^6.3.1",
    "prop-types": "^15.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.6",
    "styled-components": "^4.3.1",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:nobrowser": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "build:clean": "rm -rf ./build/",
    "clean:all": "rm -rf ./node_modules/ ./build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "cy:run": "export PORT=3006 && yarn start:nobrowser & wait-on http://localhost:3006 && cypress run --spec cypress/integration/application/**/*",
    "cy:open": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0"
  }
}
